package signature
import future.keywords.contains
import future.keywords.if

deny contains msg if {
  not input.Data.signer
  msg := "Predicate must have signer key"
}
allow[msg] {
  signer := input.Data.signer
  signer != "cosign\n"
  msg := sprintf("unexpected signer type: %v", signer)
}

allow[msg] {
  before = time.parse_rfc3339_ns("2021-11-10T17:10:27Z")
  actual = time.parse_rfc3339_ns(input.Timestamp)
  actual != before
  msg := sprintf("unexpected time: %v", [input.Timestamp])
}

